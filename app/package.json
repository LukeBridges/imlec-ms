    "@angular/core": "^12.2.2",
    "@angular/forms": "^12.2.2",
    "@angular/material": "^12.2.2",
    "@angular/platform-browser": "^12.2.2",
    "@angular/platform-browser-dynamic": "^12.2.2",
    "@angular/router": "^12.2.2",
    "@ngrx/effects": "^12.4.0",
    "@ngrx/entity": "^12.4.0",
    "@ngrx/router-store": "^12.4.0",
    "@ngrx/store": "^12.4.0",
    "@ngrx/store-devtools": "^12.4.0",
    "rxjs": "^6.5.3",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
